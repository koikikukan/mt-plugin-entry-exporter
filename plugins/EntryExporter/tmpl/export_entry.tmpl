<mtapp:listing id="entries" loop="entries" hide_pager="1" show_actions="0" type="entries">
    <mt:if __first__>
        <thead>
            <tr>
                <th class="cb"><input type="checkbox" name="id-head" value="all" class="select" /></th>
                <th class="field-name primary-col"><__trans phrase="Name"></th>
            </tr>
        </thead>
        <tbody>
    </mt:if>
            <tr class="<mt:if name="__odd__">odd<mt:else>even</mt:if>">
                <td class="cb">
                    <input type="checkbox" name="plugin_default_entries_export_ids" class="select" value="<mt:var name="entry_id">" <mt:if checked>checked="checked" </mt:if>/>
                </td>
                <td class="field-name"><mt:var name="entry_title" escape="html"></td>
            </tr>
    <mt:if __last__>
        </tbody>
    </mt:if>
</mtapp:listing>
<script type="text/javascript">
jQuery.mtCheckbox();
</script>
